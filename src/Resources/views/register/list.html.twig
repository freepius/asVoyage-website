{% set currentNav = 'register' %}

{% extends "layout.html.twig" %}

{% from "register/macros.twig" import register_render %}


{% block main_inner %}

    <header>
        <h1>{{ 'register.listing'|trans }}</h1>
    </header>

    <ul>
        {% for entry in entries %}
            <li class="alert alert-success">{{ register_render(entry) }}</li>
        {% endfor %}
    </ul>

{% endblock %}
